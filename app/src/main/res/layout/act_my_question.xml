<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:clipToPadding="true"
              android:fitsSystemWindows="true"
              android:orientation="vertical">

    <com.easemob.easeui.widget.EaseTitleBar
        android:id="@+id/title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.easemob.easeui.widget.EaseTitleBar>

    <com.easemob.easeui.widget.xlistview.XListView
        android:id="@+id/lv_question"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.easemob.easeui.widget.xlistview.XListView>
</LinearLayout>
